/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 50rpx 0;
  box-sizing: border-box;
} 

.row-list{
  width: 100%;
  display: flex;
  flex-direction:row; 
  flex-wrap: nowrap;
  margin-top: 12px;
  font-size: 14px;
  color: #333333;
  border-bottom: 1px solid #e9e9e9;
}

.row-list .flex-row{
  display: flex;
  flex: 0 0 20%;
  height: 60px;
  margin:0 auto;
}

.flex-row-colum{
  width: 100%;
  display: flex;
  flex-direction: column;
}

.title {
  font-weight:bold;
  font-size:11px;
}

.flex-column {
height:32.5px;
display:flex;
justify-content:center;
align-items:center;

}


.curent-list {
  background: #fff;
  margin-top:10px;
}
